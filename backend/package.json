{
  "name": "libre-webui-backend",
  "version": "0.1.0",
  "type": "module",
  "description": "Backend API for Libre WebUI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/ --ext .ts,.js --max-warnings 100",
    "lint:fix": "eslint src/ --ext .ts,.js --fix",
    "lint:strict": "eslint src/ --ext .ts,.js --max-warnings 0"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "uuid": "^11.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0"
  }
}
